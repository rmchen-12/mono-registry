{
    "name": "@aclink/test",
    "version": "1.1.0",
    "description": "智能硬件组测试npm",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "tag": "node ./version.js",
        "x": "npm --no-git-tag-version version major",
        "y": "npm --no-git-tag-version version minor",
        "z": "npm --no-git-tag-version version patch",
        "postversion": "npm run changelog && git add . && npm run tag",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "clean": "rimraf ./dist/ ./es/ ./lib/",
        "build": "npm run clean && cross-env NODE_ENV=production father build",
        "prepublishOnly": "npm run build",
        "postpublish": "npm run clean",
        "lint": "eslint src --fix --ext .ts,.tsx"
    },
    "keywords": [
        "aclink",
        "typescript",
        "father",
        "rollup",
        "eslint",
        "prettier"
    ],
    "files": [
        "dist",
        "lib",
        "es"
    ],
    "sideEffects": false,
    "author": "rmchen",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "http://10.1.1.217/ruimin.chen/ehome-aclink-utils.git"
    },
    "devDependencies": {
        "@types/base64-js": "^1.2.5",
        "@types/crypto-js": "^3.1.45",
        "conventional-changelog-cli": "^2.0.31",
        "cross-env": "^7.0.2",
        "rollup-plugin-filesize": "^7.0.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.2",
        "base64-js": "^1.3.1",
        "crypto-js": "^4.0.0"
    },
    "browserslist": {
        "browsers": [
            "ie>8",
            "Safari >= 6"
        ]
    }
}
